.gradle/
gradle-app.setting
!gradle-wrapper.jar
!gradle-wrapper.properties

# Maven
target/
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties
dependency-reduced-pom.xml
buildNumber.properties
.mvn/timing.properties
.mvn/wrapper/maven-wrapper.jar

# Application-specific
# ----------------------------------
# User preferences and local config
local.properties
application-local.yml
application-local.properties
.env
.env.local

# Downloaded dependencies (should be re-downloaded by Gradle)
gradle-*.zip
gradle-*-bin/
OpenJDK*/
jdk-*/

# Runtime generated data
downloads/
uploads/
cache/

# Test results
test-output/
.test/

# Database files (if any in future)
*.db
*.sqlite
*.sqlite3

# Backup files
*.backup
*.old

# ====================================
# Exceptions - DO NOT IGNORE
# ====================================

# Keep Gradle wrapper files (essential for building)
!gradle/wrapper/gradle-wrapper.jar
!gradle/wrapper/gradle-wrapper.properties
!gradle/wrapper/

# Keep source and test directories
!src/
!testsrc/

# Keep resources
!src/main/resources/
!src/main/resources/*.xml
!src/main/resources/*.properties
!src/main/resources/*.ico
!src/main/resources/*.png

# Keep build configuration files
!build.gradle
!settings.gradle
!gradle.properties
!gradlew
!gradlew.bat

# Keep scripts
!build.ps1
!run.ps1
!*.sh

# Keep manifest files
!manifest-*.txt

# Keep Launch4j configuration
!launch4j/
!launch4j/*.xml

# Keep documentation
!README.md
!USER_MANUAL.md
!LICENSE
!CONTRIBUTING.md
!*.md

# Keep Git configuration
!.gitignore
!.gitattributes
