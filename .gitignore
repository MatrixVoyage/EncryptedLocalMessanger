/dist/
/out/
/target/
bin/
.gradle/

# Compiled class files
*.class

# Package files
*.jar
*.war
*.ear
*.nar
*.tar.gz
*.zip

# Executables
*.exe
*.app
*.dmg

# Log files
# ----------------------------------
logs/
*.log
hs_err_pid*
replay_pid*

